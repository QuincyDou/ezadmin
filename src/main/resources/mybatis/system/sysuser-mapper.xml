				</if>
		       <if test="@Ognl@isNotEmpty(cstmrtype)">
					AND CSTMRTYPE like '%${cstmrtype}%'
				</if>
		       <if test="@Ognl@isNotEmpty(cmpno)">
					AND CMPNO like '%${cmpno}%'
				</if>
	       
		</where>
	</sql>

	<!-- 总记录数 -->
	<select id="count" parameterType="com.ez.system.entity.SysUser" resultType="int">
		select count(0) from sys_user
		<include refid="querywhere1"/>
	</select>
	
