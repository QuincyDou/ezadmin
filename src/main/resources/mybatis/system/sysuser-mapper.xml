<!-- 不使用namespace的话sql搜索定位会比较方便 -->
<mapper namespace="com.ez.system.dao.SysUserDao">

	<resultMap id="SysUser" type="com.ez.system.entity.SysUser">
        <result property="userno" column="USERNO"/>
        <result property="lognm" column="LOGNM"/>
        <result property="logpwd" column="LOGPWD"/>
        <result property="dptno" column="DPTNO"/>
        <result property="dptnm" column="DPTNM"/>
        <result property="rlid" column="RLID"/>
        <result property="rlnm" column="RLNM"/>
        <result property="userrelnm" column="USERRELNM"/>
        <result property="idnum" column="IDNUM"/>
        <result property="mobile" column="MOBILE"/>
        <result property="usericom" column="USERICOM"/>
        <result property="email" column="EMAIL"/>
        <result property="optype" column="OPTYPE"/>
        <result property="isused" column="ISUSED"/>
        <result property="opright" column="OPRIGHT"/>
        <result property="uptdate" column="UPTDATE"/>
        <result property="bsbsknd" column="BSBSKND"/>
        <result property="inpostn" column="INPOSTN"/>
        <result property="cstmrtype" column="CSTMRTYPE"/>
        <result property="cmpno" column="CMPNO"/>
	</resultMap>
	
	<sql id="selectId">
		USERNO,
		LOGNM,
		LOGPWD,
		DPTNO,
		DPTNM,
		RLID,
		RLNM,
		USERRELNM,
		IDNUM,
		MOBILE,
		USERICOM,
		EMAIL,
		OPTYPE,
		ISUSED,
		OPRIGHT,
		UPTDATE,
		BSBSKND,
		INPOSTN,
		CSTMRTYPE,
		CMPNO
    </sql>
    
    <select id="findAll" resultMap="SysUser">
		select
		<include refid="selectId" />
		from sys_user
	</select>
	
	<select id="queryAll" resultMap="SysUser" parameterType="com.ez.system.entity.SysUser">
		select
		<include refid="selectId" />
		from sys_user
		<include refid="querywhere1"/>
		<if test="@Ognl@isNotEmpty(sort) and @Ognl@isNotEmpty(direction)">
		 order by ${sort} ${direction}
		</if>
	</select>

	<select id="query" resultMap="SysUser" parameterType="java.util.HashMap">
		select
		<include refid="selectId" />
		from sys_user
		<include refid="querywhere"/>
		<if test="@Ognl@isNotEmpty(t.sort) and @Ognl@isNotEmpty(t.direction)">
		 order by ${t.sort} ${t.direction}
		</if>
	</select>
	
	<sql id="querywhere">
		<!-- ognl访问静态方法的表达式 为@class@method(args),以下为调用Ognl.isNotEmpty()方法,还有其它方法如isNotBlank()可以使用，具体请查看Ognl类 -->
		<where>	      				
			   <if test="@Ognl@isNotEmpty(t.lognm)">
					AND LOGNM like '%${t.lognm}%'
			   </if>
			   <if test="@Ognl@isNotEmpty(t.logpwd)">
					AND LOGPWD like '%${t.logpwd}%'
			   </if>
			   <if test="@Ognl@isNotEmpty(t.dptno)">
					AND DPTNO like '%${t.dptno}%'
			   </if>
			   <if test="@Ognl@isNotEmpty(t.dptnm)">
					AND DPTNM like '%${t.dptnm}%'
			   </if>
			   <if test="@Ognl@isNotEmpty(t.rlid)">
					AND RLID like '%${t.rlid}%'
			   </if>
			   <if test="@Ognl@isNotEmpty(t.rlnm)">
					AND RLNM like '%${t.rlnm}%'
			   </if>
			   <if test="@Ognl@isNotEmpty(t.userrelnm)">
					AND USERRELNM like '%${t.userrelnm}%'
			   </if>
			   <if test="@Ognl@isNotEmpty(t.idnum)">
					AND IDNUM like '%${t.idnum}%'
			   </if>
			   <if test="@Ognl@isNotEmpty(t.mobile)">
					AND MOBILE like '%${t.mobile}%'
			   </if>
			   <if test="@Ognl@isNotEmpty(t.usericom)">
					AND USERICOM like '%${t.usericom}%'
			   </if>
			   <if test="@Ognl@isNotEmpty(t.email)">
					AND EMAIL like '%${t.email}%'
			   </if>
			   <if test="@Ognl@isNotEmpty(t.optype)">
					AND OPTYPE like '%${t.optype}%'
			   </if>
			   <if test="@Ognl@isNotEmpty(t.isused)">
					AND ISUSED like '%${t.isused}%'
			   </if>
			   <if test="@Ognl@isNotEmpty(t.opright)">
					AND OPRIGHT like '%${t.opright}%'
			   </if>
			   <if test="@Ognl@isNotEmpty(t.uptdate)">
					AND UPTDATE like '%${t.uptdate}%'
			   </if>
			   <if test="@Ognl@isNotEmpty(t.bsbsknd)">
					AND BSBSKND like '%${t.bsbsknd}%'
			   </if>
			   <if test="@Ognl@isNotEmpty(t.inpostn)">
					AND INPOSTN like '%${t.inpostn}%'
			   </if>
			   <if test="@Ognl@isNotEmpty(t.cstmrtype)">
					AND CSTMRTYPE like '%${t.cstmrtype}%'
			   </if>
			   <if test="@Ognl@isNotEmpty(t.cmpno)">
					AND CMPNO like '%${t.cmpno}%'
			   </if>
	       
		</where>
	</sql>
	
		<sql id="querywhere1">
		<!-- ognl访问静态方法的表达式 为@class@method(args),以下为调用Ognl.isNotEmpty()方法,还有其它方法如isNotBlank()可以使用，具体请查看Ognl类 -->
		<where>	      				
		       <if test="@Ognl@isNotEmpty(lognm)">
					AND LOGNM like '%${lognm}%'
				</if>
		       <if test="@Ognl@isNotEmpty(logpwd)">
					AND LOGPWD like '%${logpwd}%'
				</if>
		       <if test="@Ognl@isNotEmpty(dptno)">
					AND DPTNO like '%${dptno}%'
				</if>
		       <if test="@Ognl@isNotEmpty(dptnm)">
					AND DPTNM like '%${dptnm}%'
				</if>
		       <if test="@Ognl@isNotEmpty(rlid)">
					AND RLID like '%${rlid}%'
				</if>
		       <if test="@Ognl@isNotEmpty(rlnm)">
					AND RLNM like '%${rlnm}%'
				</if>
		       <if test="@Ognl@isNotEmpty(userrelnm)">
					AND USERRELNM like '%${userrelnm}%'
				</if>
		       <if test="@Ognl@isNotEmpty(idnum)">
					AND IDNUM like '%${idnum}%'
				</if>
		       <if test="@Ognl@isNotEmpty(mobile)">
					AND MOBILE like '%${mobile}%'
				</if>
		       <if test="@Ognl@isNotEmpty(usericom)">
					AND USERICOM like '%${usericom}%'
				</if>
		       <if test="@Ognl@isNotEmpty(email)">
					AND EMAIL like '%${email}%'
				</if>
		       <if test="@Ognl@isNotEmpty(optype)">
